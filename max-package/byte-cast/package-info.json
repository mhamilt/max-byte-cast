{
  "name": "byte-cast",
  "version": "0.1-alpha.0",
  "author": "Matthew Hamilton",
  "description": "Max Object to cast a list of byte data such as that received from Arduino serial comms or Bluetooth Low Energy devices",
  "tags": ["serial", "bytes", "arduino"],
  "website": "https://github.com/mhamilt/max-byte-cast",
  "max_version_min": "8.0",
  "max_version_max": "none",
  "os": {
    "macintosh": {
      "min_version": "10.13",
      "platform": ["x64"]
    },
    "windows" : {
     "platform" : [ "ia32", "x64" ],
     "min_version" : "none"
 }
  },
  "homepatcher": "byte-cast-example.maxpat",
  "package_extra": {
    "reverse_domain": "com.mhamilt",
    "copyright": "GNU General Public License v3.0"
  },
  "filelist": {
    "examples": ["byte-cast-example.maxpat"],
    "externals": ["byte-cast.mxo", "byte-cast.mxe", "byte-cast.mxe64"],
    "help": ["byte-cast.maxhelp"],
    "extras": [],
    "docs": ["byte-cast.maxref.xml"]
  }
}
